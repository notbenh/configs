#!/usr/bin/perl;

use strict;
use warnings;

use Test::Most qw{no_plan};

#-----------------------------------------------------------------
#  
#-----------------------------------------------------------------
BEGIN {

use_ok('PACKAGE');
can_ok('PACKAGE', qw{
});

}

#---------------------------------------------------------------------------
#  
#---------------------------------------------------------------------------
ok( my $obj  = PACKAGE->new() );
isa_ok(  $obj, 
         'PACKAGE', 
         q{[PACKAGE] new()},
);

