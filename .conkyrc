background yes
font -artwiz-lime-*-*-*-*-*-*-*-*-*-*-*-*
update_interval 1.0
total_run_times 0
#own_window yes
#own_window_type override
#own_window_transparent yes
#own_window_hints undecorated,above,sticky,skip_taskbar,skip_pager
double_buffer yes
minimum_size 510 10
maximum_width 510
alignment top_right
gap_x 0
gap_y 2
border_width 0
border_margin 0
draw_shades yes
draw_outline no
draw_borders no
draw_graph_borders yes
default_color white
default_shade_color black
default_outline_color white
default_bar_size 5 24
default_color grey
draw_graph_borders false
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale no

TEXT
$alignr ${color #ee9a00}${time %a %d %H:%M}$color ${texeci 60 /home/benh/bin/ttim 2> /dev/null | fold -w50} ${color #ee9a00}CPU$color:${cpugraph cpu1 10,24}${cpugraph cpu2 10,24} ${color #ee9a00}RAM$color:${memgraph 10,24} ${color #ee9a00}UP$color:${upspeedgraph eth0 10,24} ${color #ee9a00}DWN$color:${downspeedgraph eth0 10,24} 
